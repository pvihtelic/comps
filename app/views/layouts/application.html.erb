<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "comps" %></title>
    <meta name="public comparable company analysis" content="">
    <meta name="pat vihtelic" content="">
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body>
    <div class="holder">
    <header class="navbar navbar-fixed-top">
      <nav class="navbar-inner">
        <div class="container">
          <%= render 'layouts/navigation' %>
        </div>
      </nav>
    </header>
    <div id="main" role="main">
      <div class="container">
        <div class="content">
           <div class="row">
            <div class="span12">
              <%= render 'layouts/messages' %>
              <%= yield %>
            </div>
          </div>
          <footer class="footer">
            <div class="navbar navbar-fixed-bottom">
              <ul class="footer-nav">
                <li class="footer-nav"><%= link_to "Created by Pat Vihtelic", "www.linkedin.com/pub/patrick-vihtelic/6/b2b/a64", :class=>"footer-nav" %></li>
              </ul>
            </div>
          </footer>
        </div>
      </div> <!--! end of .container -->
    </div> <!--! end of #main -->
    </div>
  </body>
</html>